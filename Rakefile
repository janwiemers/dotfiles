require 'rake'
require 'fileutils'
# require File.join(File.dirname(__FILE__), 'bin', 'yadr', 'vundle')

desc "install dependencies and settings"
task :install do
  print_headline('Begin Installation')
end

private

def print_headline(msg)
  puts
  puts "======================================================"
  puts "#{msg}"
  puts "======================================================"
  puts
end
